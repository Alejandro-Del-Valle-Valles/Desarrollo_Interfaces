<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RegistroUsuarios.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RegistroUsuarios.Views"
    Title="RegistroUsuarios">

    <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Registro"
        ContentTemplate="{DataTemplate local:RegisterPage}"
        Route="RegisterPage"/>

    <ShellContent
        Title="Lista de Usuarios"
        ContentTemplate="{DataTemplate local:UsersListPage}"
        Route="UsersListPage"/>

</Shell>
